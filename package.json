  "devDependencies": {
    "concurrently": "4.1.0",
    "eslint-plugin-react": "^7.16.0",
    "react-scripts": "^3.3.0"
  },
  "dependencies": {
    "autoprefixer-stylus": "0.14.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.7.12",
    "mongoose-paginate": "^5.0.3",
    "nodemon": "^2.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.8.3",
